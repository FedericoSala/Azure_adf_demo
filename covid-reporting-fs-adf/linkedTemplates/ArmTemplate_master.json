{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "covid-reporting-fs-adf"
		},
		"ls_ablob_covidereportingfssa_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_ablob_covidereportingfssa'"
		},
		"ls_adls_covidreportingfsdl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_covidreportingfsdl'"
		},
		"ls_adls_covidreportingfsdl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covidreportingfsdl.dfs.core.windows.net/"
		},
		"ls_http_opendata_ecdc_eu_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().sourceBaseUrl}"
		},
		"tr_ingest_population_data_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/1d03754b-1cdc-473e-833f-b8919f091947/resourceGroups/covid-reporting-rg/providers/Microsoft.Storage/storageAccounts/covidreportingfssa"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "covid-reporting-fs-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_ablob_covidereportingfssa_connectionString": {
						"value": "[parameters('ls_ablob_covidereportingfssa_connectionString')]"
					},
					"ls_adls_covidreportingfsdl_accountKey": {
						"value": "[parameters('ls_adls_covidreportingfsdl_accountKey')]"
					},
					"ls_adls_covidreportingfsdl_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_covidreportingfsdl_properties_typeProperties_url')]"
					},
					"ls_http_opendata_ecdc_eu_properties_typeProperties_url": {
						"value": "[parameters('ls_http_opendata_ecdc_eu_properties_typeProperties_url')]"
					},
					"tr_ingest_population_data_properties_typeProperties_scope": {
						"value": "[parameters('tr_ingest_population_data_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}